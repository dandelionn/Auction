<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
        <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
        <section name="entityFramework"
          type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
          requirePermission="false"/>
    </configSections>
  <appSettings>
    <add key="AuctionMaxPeriodInMonths" value="4"/>
    <add key="MinAuctionBeginPrice" value="10"/>
    <add key="MinAuctionBeginPriceCurrencyName" value="Euro"/>
    <add key="MaxRaisePercentage" value="0.1"/>
    <add key="MaxAuctionsStartedAndNotFinalized" value="3"/>  <!-- This is K -->
    <add key="MaxAuctionsStartedAndNotFinalizedForCategory" value="2"/> <!-- This is M -->
    <add key="InitialPersonScore" value="10"/> <!-- This is S -->
    <add key="MinimumAllowedPersonScore" value="7"/> <!-- This is P -->
    <add key="NumOfLastScoresForAverage" value = "5"/> <!-- This is N -->
    <add key="NumOfAuctionBanDays" value="10"/> <!-- This is Z -->
  </appSettings>
  <entityFramework>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
    </providers>
  </entityFramework>
</configuration>